.wrapper-inner(tabindex="0", id="game", ng-keyup="$game.keyPress($event.key)")
  span(ng-bind="$game.success" ng-show="$game.success" class="success-key")
  div(class="fails")

    p(ng-if="$game.score.fails.length") You missed:
    ul
      li(ng-repeat="letter in $game.score.fails" ng-bind="letter")

  div(class="result")
    result(word="$game.word")

  hangman
